{
  "name": "Rival",
  "description": "",
  "version": "1.0",
  "manifest_version": 2,
  "action": {
    "default_popup": "build/index.html"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [{
      "matches": ["*://amazon.com/*"],
      "js": ["content.js"]
  }],
  "permissions": ["activeTab"]
}
